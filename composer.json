{
    "name": "natsumeaurlia/auto-property-reflection",
    "description": "Automatically reflect properties.",
    "license": "MIT",
    "keywords": ["api", "reflecction", "utility", "entity"],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Natsumeaurlia\\Reflection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3 || ^9.1"
    },
    "scripts": {
        "test": "phpunit ./tests"
    },
    "minimum-stability": "stable"
}
